{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}

